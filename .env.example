# Zendesk OAuth
ZENDESK_CLIENT_ID=your-zendesk-client-id
ZENDESK_CLIENT_SECRET=your-zendesk-client-secret
ZENDESK_REDIRECT_URI=https://your-app.com/oauth/callback

# Storage (S3/R2)
STORAGE_BUCKET_NAME=your-bucket-name
STORAGE_REGION=us-east-1
STORAGE_ACCESS_KEY=your-access-key
STORAGE_SECRET_KEY=your-secret-key

# AGNO
AGNO_API_KEY=your-agno-api-key
AGNO_PROJECT_ID=your-agno-project-id

# Analytics
ANALYTICS_KEY=your-analytics-key

# App
NODE_ENV=development
PORT=3000
